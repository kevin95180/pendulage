{% extends "PendulageAccueilBundle::base.html.twig" %}

{% block title %}Catenaire{% endblock %}

{% block body %}

  <div class="panel panel-default">
      <!-- Default panel contents -->
      <div class="panel-heading">Caract√©ristiques catenaire 25KV</div>

      <!-- Table -->
      <table class="table">
          <thead>
            <tr>
                <th>Type of OHLE</th>
                <th>Messenger wire weight</th>
                <th>Messenger wire tension</th>
                <th>Contact wire weight</th>
                <th>Contact wire tension (per wire)</th>
                <th>Weight of dropper P</th>
            </tr>
          </thead>
          {% for ohle in listeOhle %}
          <tbody>
              <tr>
                  
                  <td>{{ohle.typeofohle}}</td>
                  <td>{{ohle.MessengerWireWeight}}</td>
                  <td>{{ohle.MessengerWireTension}}</td>
                  <td>{{ohle.ContactWireWeight}}</td>
                  <td>{{ohle.ContactWireTension}}</td>
                  <td>{{ohle.WeightOfDropper}}</td>
              </tr>
          </tbody>
          {% endfor %}  
      </table>
  </div>

{% endblock %}
